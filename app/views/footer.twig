{% block footer %}
    <!-- Footer Section Start -->
      <footer class="footer-area section-padding sticky-bottom">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="footer-text text-center wow fadeInDown" data-wow-delay="0.3s">
                <ul class="social-icon">
                  <li>
                    <a class="facebook" target="_blank" href="https://www.facebook.com/florent.ascensio/"><i class="icon-social-facebook"></i></a>
                  </li>
                  <li>
                    <a class="instagram" target="_blank" href="https://www.linkedin.com/in/florent-ascensio/"><i class="icon-social-linkedin"></i></a>
                  </li>
                  <li>
                    <a class="instagram" target="_blank" href="https://github.com/Flo654/"><i class="icon-social-github"></i></a>
                  </li>
                </ul>
                {% if (isConnected == true) and (isAdmin)  %}             
                <a href="/admin" >back office Administration</a>              
                {% endif %}             
              </div>
            </div>
          </div>
        </div>
      </footer>
    <!-- Footer Section End -->


    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script>$('#confirmModal').on('show.bs.modal', function (event) {
      var button = $(event.relatedTarget) // Button that triggered the modal
      var articleId = button.data('whatever') // Extract info from data-* attributes
      var modal = $(this)      
      modal.find('#testy').attr( "href", "/deleteArticle-" + articleId );
       })
    </script>
    <script>$('#confirmModal2').on('show.bs.modal', function (event) {
      var button = $(event.relatedTarget) // Button that triggered the modal
      var commentId = button.data('whatever') // Extract info from data-* attributes
      var modal = $(this)      
      modal.find('#commentId').attr( "value", commentId );
       })
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-Piv4xVNRyMGpqkS2by6br4gNJ7DXjqk09RmUpJ8jgGtD7zP9yug3goQfGII0yAns" crossorigin="anonymous"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Trumbowyg/2.23.0/trumbowyg.min.js" integrity="sha512-sffB9/tXFFTwradcJHhojkhmrCj0hWeaz8M05Aaap5/vlYBfLx5Y7woKi6y0NrqVNgben6OIANTGGlojPTQGEw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script>$('#content').trumbowyg();</script>
    
    </body>
  </html>
{% endblock %}